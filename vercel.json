{
  "version": 2,
  "name": "create-app-dashboard",
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "detail.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/detail(.*)",
      "dest": "/detail.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
